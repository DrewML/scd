#!/usr/bin/env node

const majorVersion = Number(process.version.split('v')[1].split('.')[0]);
if (majorVersion < 10) {
    console.error(
        `scd requires a version of node.js >= 10.0.0. You're currently using ${
            process.version
        }`,
    );
    process.exit(1);
}

require('source-map-support/register');
require('../dist/cli');
